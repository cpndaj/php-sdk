{
  "name": "tradenity/php-sdk",
  "version":"1.0.1",
  "license": "Apache-2.0",
  "description": "PHP interface to the Tradenity REST API, The easiest eCommerce API for web and mobile development.",
  "keywords": [
    "php",
    "sdk",
    "api"
  ],
  "authors": [
    {
      "name": "Tradenity Inc.",
      "email": "tradenity@tradenity.com",
      "homepage": "http://www.tradenity.com",
      "role": "Vendor"
    }
  ],
  "support": {
    "email": "support@tradenity.com",
    "issues": "http://github.com/tradenity/php-sdk/issues",
    "source": "http://github.com/tradenity/php-sdk"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.5",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^6.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "~2.6",
    "friendsofphp/php-cs-fixer": "~1.12"
  },
  "autoload": {
    "psr-4": {
      "Tradenity\\SDK\\": "src/tradenity/sdk"
    }
  }
}